<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!--对应Java中Mapper的全类名-->
<mapper namespace="com.chenjw.mapper.NbaTeamManagerMapper">
    <insert id="addTeam">
        insert into nba_team (team_name,team_part,team_area,team_gym)
        values(#{team.teamName}, #{team.teamPart}, #{team.teamArea}, #{team.teamGym});
    </insert>
</mapper>